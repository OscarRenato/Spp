@model IEnumerable<Spp.Data.Entities.Proyecto>
@{ ViewData["Title"] = "Index"; }

<h1>Index</h1>

<p>
    <a class="btn btn-primary" asp-action="Create"> Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProyectoNro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Empresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmpresaRepresentante)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FContrato)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionFinanc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoFinal)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FIngreso)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProyectoNro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Empresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmpresaRepresentante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FContrato)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionFinanc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoFinal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FIngreso)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a>
                <a asp-action="Details" asp-route-id="@item.Id">Details</a>
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
